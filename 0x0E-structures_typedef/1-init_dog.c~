#include "dog.h"
#include <stdio.h>

/**
 * init_dog - return name of the file asdasgassgasg
 * @d - asfgasgdshd
 * @name - adasgsag
 * @age - asasgasg
 * @owner - asgasgasg
 * Return: return always 0
 */

void init_dog(struct dog *d, char *name, float age, char *owner)
{
if (d == NULL)
return;

d->name = name;
d->age = age;
d->owner = owner;

}
