#include "holberton.h"
/**
 * char *_memset - prints buffer in hexa
 * @s: the address of memory to print
 * @b: the size of the memory to print
 * @n: int unsigned
 * Return: Nothing.
 */

char *_memset(char *s, char b, unsigned int n)
{
unsigned int n;
for (i = 0; i < n; i++)
	s[i] = b;
return (s);
}
