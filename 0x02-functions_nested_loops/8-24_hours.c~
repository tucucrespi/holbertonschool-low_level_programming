#include "holberton.h"

/**
 * jack_bauer  - print hour
 *
 * Return: On success 1.
 * On error, -1 is returned, and errno is set appropriately.
 */
void jack_bauer(void)
{
	int a;
	int b;
	int c;
	int d;
	int tope;
	tope = 9;

	for (a = 0; a <= 2; a++)
	{
		for (b = 0; b <= tope; b++)
		{
			for (c = 0; c <= 5; c++)
			{
				for (d = 0 ; d <= 9; d++)
				{
					if (a == 2)
					{
						tope = 3;
					}
					_putchar(a + '0');
					_putchar(b + '0');
					_putchar(':');
					_putchar(c + '0');
					_putchar(d + '0');
					_putchar('\n');
				}
			}
		}
	}

}
